# If you are running this on a different machine, set the following env variables.
AUGMENTOS_API_KEY=

# When the SDK is updated to include settings, this won't be needed since it will be set automatically per user based on where their client is pointng.
CLOUD_HOST_NAME= # e.g. prod.augmentos.org

PACKAGE_NAME=com.augmentos.livecaptions

# LLM Provider Configuration
LLM_PROVIDER=azure # Options: azure, openai, anthropic, google, perplexity
LLM_MODEL=gpt-4o # See LLMModel enum in LLMProvider.ts for available models

# API Keys for LLM Providers
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_API_INSTANCE_NAME=
AZURE_OPENAI_API_DEPLOYMENT_NAME=
AZURE_OPENAI_API_VERSION=2024-02-15-preview
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=
GOOGLE_PROJECT_ID=
GOOGLE_LOCATION=us-central1
PERPLEXITY_API_KEY=
